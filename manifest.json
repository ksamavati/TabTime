{
    "name": "TabTime",
    "version": "1.0.0",
    "description": "Logs time spent on websites",
    "manifest_version": 3,
    "author": "Kayvon Samavati",
    "action":{
        "default_popup": "popup.html",
        "default_title": "TabTime"
    }, 
    "background": {
        "service_worker": "background.js"
    },
	"permissions": [
		"tabs",
        "storage",
        "unlimitedStorage"
	]
}
